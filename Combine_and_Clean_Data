# ğŸ“¦ Load packages
library(tidyverse)
library(lubridate)
library(skimr)
library(janitor)
library(ggplot2)

# ğŸ“ Define path to input folder
data_path <- "/kaggle/input/20242025-divvy-bike-sharing-data-cyclistic"

# ğŸ“„ List and load all CSV files
files <- list.files(path = data_path, pattern = "*.csv", full.names = TRUE)

# ğŸ§© Combine all CSVs into one dataframe
all_trips <- files %>%
  map_df(read_csv)

# ğŸ§¼ Clean column names
all_trips <- clean_names(all_trips)

# ğŸ” Preview column names
colnames(all_trips)
